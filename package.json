{
  "name": "mini-router",
  "version": "0.1.4",
  "description": "Used with dialog-view, you can dynamically render components in the dialog instead of nested pop ups",
  "main": "lib/bundle.js",
  "module": "esm/bundle.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rollup -c rollup.config.js -w",
    "build": "rollup -c rollup.config.js",
    "check": "npm run lintfix && npm run prettier",
    "lintfix": "eslint src types --ext .ts --fix",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,ts}\"",
    "pub": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asyoulikeme/mini-router.git"
  },
